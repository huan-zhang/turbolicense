	    <param-name>ctx</param-name>
	    <param-value>@root@</param-value>
  	</context-param>
  	<context-param>
	    <param-name>download_path</param-name>
	    <param-value>@download.path@</param-value>
  	</context-param>
  	<context-param>
	    <param-name>server</param-name>
	    <param-value>@server@</param-value>
  	</context-param>
